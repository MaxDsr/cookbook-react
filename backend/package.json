{
  "version": "1.0.3",
  "description": "A boilerplate for Node.js apps / Rest API / Authentication from scratch - express, mongodb (mongoose).",
  "main": "./src",
  "nodeVersion": "18.16.0",
  "bugs": "https://github.com/watscho/express-mongodb-rest-api-boilerplate/issues",
  "keywords": [
    "express",
    "mongodb",
    "mongoose",
    "node",
    "nodejs",
    "authentication",
    "scratch",
    "boilerplate",
    "authorization",
    "docker",
    "api",
    "rest"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "nodemon",
    "build": "tsc && tsc-alias",
    "docker-bash": "docker exec -it cookbook-backend /bin/bash",
    "migrate-mongo-status": "migrate-mongo status",
    "migrate-mongo-up": "migrate-mongo up",
    "migrate-mongo-down": "migrate-mongo down",
    "seed-recipes": "docker exec -it cookbook-backend npx ts-node --files -r tsconfig-paths/register ./src/scripts/seedRecipes.ts",
    "seed-recipes-local": "npx ts-node --files -r tsconfig-paths/register ./src/scripts/seedRecipes.ts",
    "upload-recipe-images": "npx ts-node --files -r tsconfig-paths/register ./src/scripts/uploadRecipeImages.ts",
    "lint": "eslint --debug ./src",
    "lint:write": "eslint --debug ./src --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|md|ts)\"",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|json|css|md|ts)\""
  },
  "author": "Watscho Aiwasov",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/express-serve-static-core": "4.17.33",
    "@types/multer": "1.4.7",
    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "44.0.2",
    "nodemon": "^3.1.0",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.7.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "5.0.0-beta.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "http-status-codes": "2.2.0",
    "jwt-decode": "^4.0.0",
    "migrate-mongo": "^11.0.0",
    "minio": "^8.0.5",
    "mongoose": "^7.5.0",
    "multer": "1.4.5-lts.1",
    "uuid": "^13.0.0",
    "winston": "3.8.2"
  }
}
